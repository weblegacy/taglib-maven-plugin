<?xml version="1.0" encoding="UTF-8"?>
<site xmlns="http://maven.apache.org/SITE/2.0.0" 
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://maven.apache.org/SITE/2.0.0 https://maven.apache.org/xsd/site-2.0.0.xsd"
      name="${project.name}">

    <skin>
      <groupId>org.apache.maven.skins</groupId>
      <artifactId>maven-fluido-skin</artifactId>
      <version>${fludioVersion}</version>
    </skin>

    <custom>
        <fluidoSkin>
            <topBarEnabled>false</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
            <skipGenerationDate>true</skipGenerationDate>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
            <gitHub>
                <projectId>weblegacy/taglib-maven-plugin</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>black</ribbonColor>
            </gitHub>
        </fluidoSkin>
    </custom>

    <body>
        <menu name="Overview">
            <item name="Introduction" href="index.html"/>
            <item name="Usage" href="usage.html"/>
            <item name="Goals" href="plugin-info.html"/>
            <item name="FAQ" href="faq.html"/>
        </menu>

        <menu name="Configuration">
            <item name="taglib:taglibdoc" href="taglibdoc-mojo.html"/>
            <item name="taglib:taglibdocjar" href="taglibdocjar-mojo.html"/>
            <item name="taglib:tagreference" href="tagreference-mojo.html"/>
            <item name="taglib:tldgenerate" href="tldgenerate-mojo.html"/>
            <item name="taglib:validate" href="validate-mojo.html"/>
        </menu>

        <menu ref="reports"/>

<!--
        <footer>
            A maven plugin for jsp tag libraries developers. It eases the
            generation, documentation and testing of jsp tag libraries.
        </footer>
-->

    </body>
</site>